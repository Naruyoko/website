		mes "You need make at least "+notation(powf(legacyLevel+1,2)*1000000000-cookiesTotal)+"\nCookies more to ascent!"
	}else{
		font "Comic Sans MS",18
		pos 50,100
		mes "If you ascent, you'll lose everything but rainbow cookies,\nsuper structures, lifetime cookies, achievements, blackhole,\ngems, borderless cookies, and legacy level."
		mes "If you ascent now, you'll get additionally\n"+notation(floor(sqrt(cookiesTotal/1000000000))-legacyLevel)+" borderless cookies/legacy level, total of\n"+notation(floor(sqrt(cookiesTotal/1000000000))-legacyLevel+borderlessCookies)+" borderless cookies and "+ntation(floor(sqrt(cookiesTotal/1000000000)))+" legacy level."
		chkbox "Ascent?",ascentConfirm(0)
		chkbox "Ascent?",ascentConfirm(1)
		chkbox "Ascent?",ascentConfirm(2)
		button "Ascent!",*ascention
	}
	font "Comic Sans MS",12,
	pos 620,460
	objsize 20,20
	button "X",*home
	redraw 1
	while 1
	idle
	await 50
	wend
*ascention
	if (ascentConfirm(0) and ascentConfirm(1) and ascentConfirm(2))=0:dialog "Check ALL to ascent!",0,"Not Confirmed":goto *ascent
	borderlessCookies+=floor(sqrt(cookiesTotal/1000000000))-legacyLevel
	legacyLevel=floor(sqrt(cookiesTotal/1000000000))
	cookies=double(0)
	cookiesSessionTotal=double(0)
	baseCookiesPerSecond=double(0)
	baseCookiesPerClick=double(0)
	ascents++
	dim placeholder,2
	repeat 32
		repeat 5
			amount(0,placeholder(0),placeholder(1))=0
			placeholder(1)++
		loop
		placeholder(1)=0
		placeholder(0)++
	loop
	dim placeholder,2
	repeat 32
		repeat 5
			amount(1,placeholder(0),placeholder(1))=0
			placeholder(1)++
		loop
		placeholder(1)=0
		placeholder(0)++
	loop
	dim ascentionConfirm,3
	dialog "Restart the game to end ascention",0,"Ascention"
	vsave "files\\saves\\file.txt"
	end
*status
	redraw 0
	clrobj
	color 255,255,255
	boxf
	color 0,0,0
	font "Comic Sans MS",18,
	log=""
	if cheated=1:log="*** It's not reliable as this save file has been hacked! ***"
	log+="Cookies Stored: "+notation(cookies)+" Cookies\nTotal Cookies Baked(on this session): "+notation(cookiesSessionTotal)+" Cookies\nTotal Cookies Baked(lifetime): "+notation(cookiesTotal)+" Cookies\nCookies Per Second: "
	if cookiesPerSecond<1000:log+=strf("%.2f",cookiesPerSecond):else:log+=notation(cookiesPerSecond)
	log+=" Cookies\nCookies Per Click: "+notation(cookiesPerClick)+" Cookies\nRainbow Cookies in Hand: "+notation(rainbowCookies)+" Rainbow Cookies\nGems in Hand: "+notation(gems)+" Gems\nStructures Owned: "
	dim placeholder,3
	repeat 32
		repeat 5
			placeholder(2)+=amount(0,placeholder(0),placeholder(1))
			placeholder(1)++
		loop
		placeholder(1)=0
		placeholder(0)++
	loop
	log+=str(placeholder(2))+"\nCookies Invented: "
	dim placeholder,3
	repeat 32
		repeat 5
			placeholder(2)+=amount(1,placeholder(0),placeholder(1))
			placeholder(1)++
		loop
		placeholder(1)=0
		placeholder(0)++
	loop
	log+=str(placeholder(2))+"\nBoost From Super Buildings: "+notation(superBuildingBoost)+"%\nSpecial Buildings Owned: "
	dim placeholder,3
	repeat 32
		repeat 5
			placeholder(2)+=amount(2,placeholder(0),placeholder(1))
			placeholder(1)++
		loop
		placeholder(1)=0
		placeholder(0)++
	loop
	log+=str(placeholder(2))+"\nAchievements Earned: "
	dim placeholder,3
	repeat 128
		repeat 64
			if achievement(placeholder(0),placeholder(1))=1:placeholder(2)++
			placeholder(1)++
		loop
		placeholder(1)=0
		placeholder(0)++
	loop
	log+=str(placeholder(2))+"\nSection Time: "
	sdim placeholder
	if sectionTime>=31557600:placeholder(0)+=str(int(sectionTime/31557600))+"yr "
	if sectionTime>=2629800:placeholder(0)+=str(int(sectionTime\31557600/2629800))+"mon "
	if sectionTime>=86400:placeholder(0)+=str(int(sectionTime\2629800/86400))+"d "
	if sectionTime>=3600:placeholder(0)+=str(int(sectionTime\86400/3600))+"hr "
	if sectionTime>=60:placeholder(0)+=str(int(sectionTime\3600/60))+"min "
	if sectionTime>=1:placeholder(0)+=str(int(sectionTime\60/1))+"s"
	log+=placeholder(0)+"\nTotal Time: "
	sdim placeholder
	if totalTime>=31557600:placeholder(0)+=str(int(totalTime/31557600))+"yr "
	if totalTime>=2629800:placeholder(0)+=str(int(totalTime\31557600/2629800))+"mon "
	if totalTime>=86400:placeholder(0)+=str(int(totalTime\2629800/86400))+"d "
	if totalTime>=3600:placeholder(0)+=str(int(totalTime\86400/3600))+"hr "
	if totalTime>=60:placeholder(0)+=str(int(totalTime\3600/60))+"min "
	if totalTime>=1:placeholder(0)+=str(int(totalTime\60/1))+"s"
	log+=placeholder(0)+"\nDaily Bonuses Opened: "+str(dailyBonuses)+"\nCurrent Daily Bonus Streak: Day "+str(dailyBonusStreak)+"\nHighest Daily Bonus Streak: Day"+str(highestDailyBonusStreak)+"\nTime Travels: "+str(timeTravels)+" Times\nHours Time Traveled: "+notation(hoursTimeTraveled)+" hr\nBlack Hole: Lv."+str(blackHoleLevel)+"    "+str(blackHoleExperience)+"/100\nAscentions: "+notation(ascents)+" Times\nCookies Sacrificed by Ascention: "+notation(cookiesTotal-cookiesSessionTotal)+" Cookies\nBorderless Cookies: "+notation(borderlessCookies)+"\nLegacy Level: "+notation(legacyLevel)+"\nBoost From Legacy: "+notation(legacyLevel*2)+"%"
	pos 0,0
	mesbox log,640,462,0
	pos 620,460
	objsize 20,20
	font "Comic Sans MS",12,
	button "X",*home
	redraw 1
	while 1
	idle
	await 50
	wend
*achievements
	redraw 0
	clrobj
	color 255,255,255
	boxf
	color 0,0,0
	font "Comic Sans MS",14,
	dim placeholder,2
	log=""
	if cheated=1:log="*** It's not reliable as this save file has been hacked! ***"
	repeat 3
		repeat 13
			if achievement(placeholder(0),placeholder(1))=1:log+=achievementName(placeholder(0),placeholder(1),0)+"\n"+achievementName(placeholder(0),placeholder(1),1)+"\n\n"
			placeholder(1)++
		loop
		placeholder(1)=0
		placeholder(0)++
	loop
	mesbox log,640,460,0
	pos 620,460
	objsize 20,20
	font "Comic Sans MS",12,
	button "X",*home
	redraw 1
	while 1
	idle
	await 50
	wend
*credit
	redraw 0
	clrobj
	color 255,255,255
	boxf
	color 0,0,0
	font "Comic Sans MS",18,
	pos 0,0
	sdim placeholder,1
	placeholder(0)="--Credits--\n-Program-\nNaruyoko\n-Pictures-\nNaruyoko\n[REDACTED]\n-Sound Effects-\n - http://maoudamashii.jokersounds.com/"
	mesbox placeholder(0),640,460,0
	pos 620,460
	objsize 20,20
	font "Comic Sans MS",12,
	button "X",*home
	redraw 1
	while 1
	idle
	await 50
	wend
*sound
	if mute=0:mute=1:else:mute=0
	if mute=0:mmplay 6
	goto *home