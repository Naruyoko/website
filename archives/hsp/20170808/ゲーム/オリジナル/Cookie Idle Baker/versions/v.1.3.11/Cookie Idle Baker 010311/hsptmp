	if stat=6:vsave "files\\saves\\file.txt"
	end
*exit
	lastTime=double(31557600)*gettime(0)+double(2529800)*gettime(1)+double(86400)*gettime(3)+double(3600)*gettime(4)+double(60)*gettime(5)+gettime(6)+0.001*gettime(7)
	lastDay=gettime(3)
	if gettime(1)>=2:lastDay+=31
	if gettime(1)>=3{
		lastDay+=28
		if (gettime(0)\4=0 and gettime(0)\100!=0 or gettime(0)\400=0):lastDay++
	}
	if gettime(1)>=4:lastDay+=31
	if gettime(1)>=5:lastDay+=30
	if gettime(1)>=6:lastDay+=31
	if gettime(1)>=7:lastDay+=30
	if gettime(1)>=8:lastDay+=31
	if gettime(1)>=9:lastDay+=31
	if gettime(1)>=10:lastDay+=30
	if gettime(1)>=11:lastDay+=31
	if gettime(1)>=12:lastDay+=30
	lastYear=gettime(0)
	vsave "files\\saves\\file.txt"
	end
