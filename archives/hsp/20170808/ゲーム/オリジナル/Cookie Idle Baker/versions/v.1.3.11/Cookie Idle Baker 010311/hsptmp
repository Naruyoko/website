	cookies+=boost((currentTime-lastTime)*baseCookiesPerSecond/10)
	cookiesSessionTotal+=boost((currentTime-lastTime)*baseCookiesPerSecond/10)
	cookiesTotal+=boost((currentTime-lastTime)*cookiesPerSecond/10)
	if (currentDay-1>=lastDay and currentYear=lastYear)or(lastDay=365 and (gettime(0)\100=0 or gettime(0)\400!=0))or(lastDay=366 and (gettime(0)\4=0 and gettime(0)\100!=0 or gettime(0)\400=0)){
		if (currentDay-1=lastDay and currentYear=lastYear)or(currentDay=1 and lastDay=365 and (gettime(0)\100=0 and gettime(0)\400!=0))or(currentDay=1 and lastDay=366 and (gettime(0)\4=0 and gettime(0)\100!=0 or gettime(0)\400=0)):dailyBonusStreak++:else:dailyBonusStreak=1
		dailyBonuses++
		if dailyBonusStreak>highestDailyBonusStreak:highestDailyBonusStreak=dailyBonusStreak
		font "Comic Sans MS",24
		mes "Recieved day "+str(dailyBonusStreak)+" daily bonus! Earned:"
		font "Comic Sans MS",18
