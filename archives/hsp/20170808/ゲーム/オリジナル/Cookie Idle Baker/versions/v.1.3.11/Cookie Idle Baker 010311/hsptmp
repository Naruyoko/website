	if currentTime-lastTime>=86400:placeholder(0)+=str(int((currentTime-lastTime)\2629800/86400))+"d "
	if currentTime-lastTime>=3600:placeholder(0)+=str(int((currentTime-lastTime)\86400/3600))+"hr "
	if currentTime-lastTime>=60:placeholder(0)+=str(int((currentTime-lastTime)\3600/60))+"min "
	if currentTime-lastTime>=1:placeholder(0)+=str(int((currentTime-lastTime)\60/1))+"s "
	if currentTime-lastTime<1:placeholder(0)+="<1s"
