#defcfunc notation double variable
	sdim placeholder
	if variable<1000:placeholder(0)=str(int(variable))
	if variable>=1000 and variable<1000000{
		placeholder(0)=str(int(variable/1000))+","+str(int(variable\1000))
		if variable\1000<100:placeholder(0)=str(int(variable/1000))+",0"+str(int(variable\1000))
		if variable\1000<10:placeholder(0)=str(int(variable/1000))+",00"+str(int(variable\1000))
	}
	if variable>=1000000 and variable<1000000000:placeholder(0)=strf("%.3f",variable/1000000)+" Million"
	if variable>=1000000000 and variable<1e+12:placeholder(0)=strf("%.3f",variable/1000000000)+" Billion"
	if variable>=1e+12 and variable<1e+15:placeholder(0)=strf("%.3f",variable/1e+12)+" Trillion"
	if variable>=1e+15 and variable<1e+18:placeholder(0)=strf("%.3f",variable/1e+15)+" Quadrillion"
	if variable>=1e+18 and variable<1e+21:placeholder(0)=strf("%.3f",variable/1e+18)+" Quintillion"
	if variable>=1e+21 and variable<1e+24:placeholder(0)=strf("%.3f",variable/1e+21)+" Sextillion"
	if variable>=1e+24 and variable<1e+27:placeholder(0)=strf("%.3f",variable/1e+24)+" Septillion"
	if variable>=1e+27 and variable<1e+30:placeholder(0)=strf("%.3f",variable/1e+27)+" Octillion"
	if variable>=1e+30 and variable<1e+33:placeholder(0)=strf("%.3f",variable/1e+30)+" Nonillion"
	if variable>=1e+33 and variable<1e+36:placeholder(0)=strf("%.3f",variable/1e+33)+" Deciillion"
	if variable>=1e+36 and variable<1e+39:placeholder(0)=strf("%.3f",variable/1e+36)+" Undecillion"
	if variable>=1e+39 and variable<1e+42:placeholder(0)=strf("%.3f",variable/1e+39)+" Duodecillion"
	if variable>=1e+42 and variable<1e+45:placeholder(0)=strf("%.3f",variable/1e+42)+" Tredecillion"
	if variable>=1e+45 and variable<1e+48:placeholder(0)=strf("%.3f",variable/1e+45)+" Quattordecillion"
	if variable>=1e+48 and variable<1e+51:placeholder(0)=strf("%.3f",variable/1e+48)+" Quindeciillion"
	if variable>=1e+51 and variable<1e+54:placeholder(0)=strf("%.3f",variable/1e+51)+" Sexdeciillion"
	if variable>=1e+54 and variable<1e+57:placeholder(0)=strf("%.3f",variable/1e+54)+" Septendeciillion"
	if variable>=1e+57 and variable<1e+60:placeholder(0)=strf("%.3f",variable/1e+57)+" Octodeciillion"
	if variable>=1e+60 and variable<1e+63:placeholder(0)=strf("%.3f",variable/1e+60)+" Novemdeciillion"
	if variable>=1e+63 and variable<1e+66:placeholder(0)=strf("%.3f",variable/1e+63)+" Vigintillion"
	return placeholder(0)
#defcfunc boost double variable
	dim placeholder
	placeholder=variable
	placeholder*=double(1)+superBuildingBoost/100
	placeholder*=double(1)+double(blackHoleLevel)/100
	placeholder*=double(1)+legacyLevel/50
	if amount(3,0,0)=1:placeholder*=2
	if amount(3,0,1)=1:placeholder*=3
	if amount(3,0,2)=1:placeholder*=4
	if amount(3,0,3)=1:placeholder*=6
	if amount(3,0,4)=1:placeholder*=8
	if amount(3,0,5)=1:placeholder*=10
	return placeholder
*boosterLoot
	rndf_get boosterChance
	boosterChance*=1.05
	if boosterChance<0.4{
		dim placeholder,1
		placeholder(0)=rnd(61)+120
		boosterType="Lucky"
		boosterTip="Tip:Don't tell anyone"
		if dynamicCookiesPerSecond*placeholder(0)<13{
			cookies+=13
			cookiesSessionTotal+=13
			cookiesTotal+=13
			boosterDescription="Found 13 cookies"
		}else{
			cookies+=dynamicCookiesPerSecond*placeholder(0)
			cookiesSessionTotal+=dynamicCookiesPerSecond*placeholder(0)
			cookiesTotal+=dynamicCookiesPerSecond*placeholder(0)
			boosterDescription="Found "+notation(dynamicCookiesPerSecond*placeholder(0))+" cookies"
		}
	}
	if boosterChance>=0.4 and boosterChance<0.8{
		boosterType="Hard Working"
		boosterDescription="Cookie production doubles for 100 seconds"
		boosterTip="Why isn't anyone complain about it...?"
		hardWorkingTime=2000
	}
	if boosterChance>=0.8 and boosterChance<1{
		dim placeholder,1
		placeholder(0)=rnd(31)+20
		boosterType="Rainbow"
		boosterDescription="Found "+placeholder(0)+" Rainbow Cookies"
		boosterTip="So... Shiny..."
		rainbowCookies+=placeholder(0)
	}
	if boosterChance>=1{
		boosterType="Broken Pieces"
		boosterDescription="CpC ducuples for 15 seconds"
		boosterTip="But isn't pieces are smaller than whole?"
		hardWorkingTime=300
	}
	gsel 2,1
	redraw 0
	clrobj
